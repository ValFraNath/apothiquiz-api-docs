            "group": "200",
            "type": "Number",
            "optional": false,
            "field": "question.type",
            "description": "<p>Type of the question</p>"
          },
          {
            "group": "200",
            "type": "String",
            "optional": false,
            "field": "question.subject",
            "description": "<p>Question subject</p>"
          },
          {
            "group": "200",
            "type": "Number",
            "optional": false,
            "field": "question.goodAnswer",
            "description": "<p>The good answer index</p>"
          },
          {
            "group": "200",
            "type": "Array[String]",
            "optional": false,
            "field": "question.answers",
            "description": "<p>All answers possible</p>"
          },
          {
            "group": "200",
            "type": "String",
            "optional": false,
            "field": "question.wording",
            "description": "<p>The question wording</p>"
          }
        ]
      }
    },
    "error": {
      "fields": {
        "400": [
          {
            "group": "400",
            "optional": false,
            "field": "BadRequest",
            "description": "<p>Bad request body / parameters</p>"
          }
        ],
        "404": [
          {
            "group": "404",
            "optional": false,
            "field": "NotFound",
            "description": "<p>Incorrect type of question</p>"
          }
        ],
        "422": [
          {
            "group": "422",
            "optional": false,
            "field": "NotEnoughData",
            "description": "<p>There is not enough data to generate the question</p>"
          }
        ]
      }
    },
    "version": "0.0.0",
    "filename": "../server/controllers/question.js",
    "groupTitle": "Question"
  },
